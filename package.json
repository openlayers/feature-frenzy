{
  "name": "ol-feature-frenzy",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint examples",
    "test": "npm run lint",
    "build": "slidev build",
    "start": "slidev",
    "export": "slidev export"
  },
  "dependencies": {
    "@iconify-json/twemoji": "^1.2.1",
    "@slidev/cli": "^0.50.0-beta.5",
    "@slidev/theme-default": "^0.25.0",
    "colormap": "^2.3.2",
    "eslint": "^8.57.1",
    "eslint-config-openlayers": "^19.0.0",
    "ol": "^10.2.2-dev.1730987055849",
    "proj4": "^2.14.0",
    "vue": "^3.5.12"
  },
  "eslintConfig": {
    "extends": "openlayers",
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "\\?url"
          ]
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^map"
        }
      ]
    }
  }
}
