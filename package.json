{
  "name": "feature-frenzy",
  "version": "6.7.0",
  "scripts": {
    "lint": "eslint src",
    "test": "npm run lint",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "deploy": "rm -rf dist && npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openlayers/feature-frenzy.git"
  },
  "eslintConfig": {
    "extends": "openlayers",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "@parcel/bundler-default": {
    "minBundles": 1,
    "minBundleSize": 3000000,
    "maxParallelRequests": 20
  },
  "dependencies": {
    "colormap": "^2.3.2",
    "drag-drop": "^7.2.0",
    "ol": "^6.8.0",
    "ol-hashed": "^2.1.0",
    "ol-mapbox-style": "^6.5.1",
    "proj4": "^2.7.5"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "eslint": "^7.32.0",
    "eslint-config-openlayers": "^15.1.0",
    "gh-pages": "^3.2.3",
    "parcel": "^2.0.0-rc.0"
  }
}
